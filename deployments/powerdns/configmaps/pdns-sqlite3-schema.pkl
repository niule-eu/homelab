import "package://pkg.pkl-lang.org/pkl-k8s/k8s@1.2.1#/K8sResource.pkl"
import "package://pkg.pkl-lang.org/pkl-k8s/k8s@1.2.1#/api/core/v1/ConfigMap.pkl"

resources: Listing<K8sResource> = new {
  new ConfigMap {
    metadata {
      name = "pdns-sqlite3-schema"
    }
    data {
      ["schema.sql"] = read("https://raw.githubusercontent.com/PowerDNS/pdns/refs/heads/master/modules/gsqlite3backend/schema.sqlite3.sql").text
    }
  }
}