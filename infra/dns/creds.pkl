ovh {
  TYPE = "OVH"
  `app-key` = read("sops:/OVH_API_DNSCONTROL_KEY").text
  `app-secret-key` = read("sops:/OVH_API_DNSCONTROL_SECRET").text
  `consumer-key` = read("sops:/OVH_API_DNSCONTROL_CONSUMER_KEY").text
  endpoint = "eu"
}

powerdns {
  TYPE = "POWERDNS"
  apiKey = read("sops:/PDNS_API_KEY").text
  apiUrl = "https://pdnsapi.infra.ams23.niule.xyz"
  serverName = "localhost"
}

output {
  renderer = new JsonRenderer {}
}